<div class="feature_boxes container d-block fr-green_">
  <div class="feature_title fr-blue_">
      	<%if(el.primary.titel[0] && el.primary.titel[0].text != "") {%>
      	<h2><%=el.primary.titel[0].text%></h2>
      	<div class="separator  small left  " style="border-color: #ea6c12;border-width:4px;width:45px;margin-top:15px;margin-bottom:15px"></div>
      	<%}%>
  </div>
      	<div class="d-flex flex-wrap flex-row" >  
      	

        <% if(el.primary.columns == "3-spaltig") { %> 	
    	<% el.items.forEach(function(box, i) { %>
    		<div class="col-lg-4 col-sm-12 featurebox-text fr-red_" style="">   
    			<img src="<%=box.icon_image.small74.url%>" class="m-3"/> 			
    			<h5><%-PrismicDOM.RichText.asText(box.feature_titel)%></h5>
    			<%if(el.lineabove = "yes") { %> 
    			<div class="separator  small left " style="border-color: #ea6c12;border-width:4px;width:45px;margin-top:15px;margin-bottom:15px"></div>
    			<%}%>
          <div class="d-block h-100" style="">
    			<p><%-PrismicDOM.RichText.asHtml(box.feature_text, ctx.linkResolver, ctx.htmlSerializer)%></p>
        </div>
        
    		</div>

    	<%});%>
    	<%}%> <!-- if 3 Spalten -->




    	<% if(el.primary.columns == "2-spaltig") { %> 	
    	<% el.items.forEach(function(box, i) { %>
    		<div class="col-lg-6 col-sm-12 featurebox-text d-block" style="min-height: 275px;">  
    			<hr/> 
    			<img src="<%=box.icon_image.small74.url%>" class="m-3"/> 			
    			<h5><%-PrismicDOM.RichText.asText(box.feature_titel)%></h5>
    			<%if(el.lineabove = "yes") { %> 
    			<div class="separator  small left  " style="border-color: #ea6c12;border-width:4px;width:45px;margin-top:15px;margin-bottom:15px"></div>
    			<%}%>
                    <div class="d-block">

    			<p><%-PrismicDOM.RichText.asHtml(box.feature_text, ctx.linkResolver, ctx.htmlSerializer)%></p>
        </div>      
    		</div>
    	<%});%>
    	<%}%> <!-- if 2 Spalten -->


    	</div>
      </div>